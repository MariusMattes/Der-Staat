<gesetze>
    <gesetz>
        <id>1</id>
        <titel>Geschwindigkeitsbegrenzung einhalten</titel>
        <beschreibung>Man darf nicht schneller als die erlaubte Geschwindigkeit fahren.</beschreibung>
    </gesetz>
    <gesetz>
        <id>2</id>
        <titel>Öffentliche Ruhestörung verboten</titel>
        <beschreibung>Man darf die Öffentlichkeit nicht stören.</beschreibung>
    </gesetz>
    <gesetz>
        <id>3</id>
        <titel>Mord verboten</titel>
        <beschreibung>Es ist verboten, eine andere Person vorsätzlich zu töten.</beschreibung>
    </gesetz>
    <gesetz>
        <id>4</id>
        <titel>Diebstahl verboten</titel>
        <beschreibung>Es ist verboten, fremdes Eigentum zu stehlen.</beschreibung>
    </gesetz>
    <gesetz>
        <id>5</id>
        <titel>Drogenhandel verboten</titel>
        <beschreibung>Der Verkauf illegaler Drogen ist verboten.</beschreibung>
    </gesetz>
    <gesetz>
        <id>6</id>
        <titel>Fahren ohne Führerschein verboten</titel>
        <beschreibung>Man darf kein Fahrzeug ohne gültigen Führerschein fahren.</beschreibung>
    </gesetz>
    <gesetz>
        <id>7</id>
        <titel>Illegale Müllentsorgung verboten</titel>
        <beschreibung>Müll darf nicht illegal entsorgt werden.</beschreibung>
    </gesetz>
    <gesetz>
        <id>8</id>
        <titel>Korruption verboten</titel>
        <beschreibung>Bestechung und Korruption sind verboten.</beschreibung>
    </gesetz>
</gesetze>
